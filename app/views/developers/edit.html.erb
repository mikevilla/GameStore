<h1>Editing developer</h1>

<% form_for(@developer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label "Developer Name" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :year_established %><br />
    <%= f.text_field :year_established %>
  </p>
  <p>
    <%= f.label :about %><br />
    <%= f.text_area :about %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </p>
  <p>
    <%= f.label :updated_at %><br />
    <%= f.datetime_select :updated_at %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @developer %> |
<%= link_to 'Back', developers_path %>